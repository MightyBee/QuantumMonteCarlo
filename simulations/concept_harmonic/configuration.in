% Physical parameters
N_part		= 1		% number of particles
beta			= 1
mass			= 1		% this one goes in the harmonic potential
m1				= 1


% At the beginning, min and max random positions:
pos_min	= 0
pos_max	= 0

% Numerical parameters
N_slices			= 10
h					= 1
idrate			= 0.5		% not used
N_sweeps			= 100
N_thermal 		= 0
n_stride			= 20
p_local			= 1		% fraction of times that N_slices local moves are tried
p_displacement	= 1		% fraction of times that one global displacement is tried
p_bissection	= 1		% fraction of times that one bisscetion is tried
s_bissection	= 0.3		% relative size of the bissection compared to the size of the path
p_swap			= 1
p_inverse		= 1
p_symmetryCM	= 1

output = output2


% Potentials (harmonic, double or square)
V_ext =	harmonic
V_int =	null

omega	= 10		%harmonic

x0		= 3		%down here ref. values
V0		= 1
L		= 2

k		= 4		% spring. l0 units of x, k untouched
l0		= 2

R		= 2.9		% H-bond
